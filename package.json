{
  "name": "nodebaseapp",
  "version": "0.0.0",
  "description": "node server-side app",
  "main": "lib/index.js",
  "license": "",
  "typings": "lib/index",
  "files": [
    "lib"
  ],
  "engines": {
    "node": "^14.15.0"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "shx rm -fR ./lib",
    "compile": "tsc",
    "postinstall": "tsc",
    "db:create": "node src/database/create-db.js",
    "db:drop": "node src/database/drop-db.js",
    "db:restart": "npm run db:drop && npm run db:create && npm run db:populate",
    "db:populate": "npm run build && node lib/database/populate-db.js",
    "start": "npm run build && node lib/index.js",
    "serve": "npm run watch",
    "watch": "nodemon --watch \"src/**\" --ext \"ts,json\"  --exec \"ts-node src/index.ts\"",
    "dev": "yarn && npm run build && npm run serve"
  },
  "dependencies": {
    "@types/multer": "^1.4.7",
    "@vitalets/google-translate-api": "^7.0.0",
    "@vitalets/google-translate-token": "^1.2.0",
    "aws-sdk": "^2.988.0",
    "bcrypt": "latest",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "generate-password": "^1.6.1",
    "jwt-decode": "^3.1.2",
    "jwt-simple": "^0.5.6",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "nodemailer": "^6.6.3",
    "ntypescript": "1.201706190042.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pg-promise": "^10.11.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "latest",
    "tsc": "^2.0.3",
    "typeorm": "0.3.7",
    "winston": "latest"
  },
  "devDependencies": {
    "@types/bcrypt": "latest",
    "@types/bluebird": "^3.5.36",
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jwt-simple": "^0.5.33",
    "@types/moment": "^2.13.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.9.1",
    "@types/nodemailer": "^6.4.4",
    "@types/pg-promise": "^5.4.3",
    "@types/sequelize": "latest",
    "@types/winston": "latest",
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "latest",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
